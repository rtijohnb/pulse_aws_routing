if [ 2 -eq `ps auxw|grep 'rtiroutingservice.*PulseWis'|wc -l` ];
then
     echo "It looks like routing service is already running, try killing it first"
     echo "     kill `ps axw|grep 'rtiroutingservice.*PulseWis'|awk '{$1=$1}1'|cut -d ' ' -f 1|head -n 1`"
     exit
fi
echo "Starting routing service"
$NDDSHOME/bin/rtiroutingservice -cfgFile WIS_server_routing_config.xml -cfgName PulseWis &

